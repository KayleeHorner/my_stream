<div class="container">
  <div class="row center">
    <p>
    <h2>Watch List</h2>

    <table class="highlight responsive-table centered">
      <thead>
        <tr>
          <th>Movie Title</th>
          <th>Rental</th>
          <th>Have you Watched It?</th>
        </tr>
      </thead>
      <tbody>
        {{#each movie}}
        {{#unless watched}}
        <tr>
          <td>{{title}}</td>
          <td>{{rent}}</td>
          <td> {{>watch-change watched=false}}</td>
          {{/unless}}
          {{/each}}
        </tr>
      </tbody>
    </table>
    </p>

  </div>
</div>

<div class="parallax-container">
  <div class="parallax"><img src="/img/parallax_img.png">
  </div>
</div>

<div class="section">
  <div class="row center">
    <p>
    <h2>Already Watched</h2>
    <ul>

      {{#each movie}}
      {{#if watched}}
      <li> {{title}} </li>
      {{/if}}
      {{/each}}

    </ul>
    </p>

  </div>
</div>